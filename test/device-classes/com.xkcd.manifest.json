{
    "module_type": "org.thingpedia.v2",
    "params": {},
    "category": "data",
    "subcategory": "media",
    "types": [],
    "child_types": [],
    "auth": {
        "type": "none"
    },
    "queries": {
        "get_comic": {
            "args": [
                {
                    "name": "number",
                    "type": "Number",
                    "question": "What Xkcd comic do you want?",
                    "is_input": true,
                    "required": false
                },
                {
                    "name": "title",
                    "type": "String",
                    "question": "",
                    "is_input": false,
                    "required": false
                },
                {
                    "name": "picture_url",
                    "type": "Entity(tt:picture)",
                    "question": "",
                    "is_input": false,
                    "required": false
                },
                {
                    "name": "link",
                    "type": "Entity(tt:url)",
                    "question": "",
                    "is_input": false,
                    "required": false
                },
                {
                    "name": "alt_text",
                    "type": "String",
                    "question": "",
                    "is_input": false,
                    "required": false
                }
            ],
            "doc": "retrieve the comic with a given number, or the latest comit",
            "confirmation": "an Xkcd comic",
            "confirmation_remote": "",
            "canonical": "xkcd comic",
            "formatted": [
                {
                    "type": "rdl",
                    "webCallback": "${link}",
                    "displayTitle": "${title}"
                },
                {
                    "type": "picture",
                    "url": "${picture_url}"
                },
                {
                    "type": "text",
                    "text": "${alt_text}"
                }
            ],
            "poll_interval": 86400000,
            "is_list": false
        },
        "random_comic": {
            "args": [
                {
                    "name": "number",
                    "type": "Number",
                    "question": "",
                    "is_input": false,
                    "required": false
                },
                {
                    "name": "title",
                    "type": "String",
                    "question": "",
                    "is_input": false,
                    "required": false
                },
                {
                    "name": "picture_url",
                    "type": "Entity(tt:picture)",
                    "question": "",
                    "is_input": false,
                    "required": false
                },
                {
                    "name": "link",
                    "type": "Entity(tt:url)",
                    "question": "",
                    "is_input": false,
                    "required": false
                },
                {
                    "name": "alt_text",
                    "type": "String",
                    "question": "",
                    "is_input": false,
                    "required": false
                }
            ],
            "doc": "retrieve a random xkcd",
            "confirmation": "a random Xkcd comic",
            "confirmation_remote": "",
            "canonical": "random xkcd comic",
            "formatted": [
                {
                    "type": "rdl",
                    "webCallback": "${link}",
                    "displayTitle": "${title}"
                },
                {
                    "type": "picture",
                    "url": "${picture_url}"
                },
                {
                    "type": "text",
                    "text": "${alt_text}"
                }
            ],
            "poll_interval": -1,
            "is_list": false
        },
        "what_if": {
            "args": [
                {
                    "name": "title",
                    "type": "String",
                    "question": "",
                    "is_input": false,
                    "required": false
                },
                {
                    "name": "link",
                    "type": "Entity(tt:url)",
                    "question": "",
                    "is_input": false,
                    "required": false
                },
                {
                    "name": "updated_time",
                    "type": "Date",
                    "question": "",
                    "is_input": false,
                    "required": false
                }
            ],
            "doc": "retrieve the latest posts on Xkcd's What If blog",
            "confirmation": "Xkcd's What If blog posts",
            "confirmation_remote": "",
            "canonical": "xkcd what if blog posts",
            "formatted": [
                {
                    "type": "rdl",
                    "webCallback": "${link}",
                    "displayTitle": "${title}"
                }
            ],
            "poll_interval": 86400000,
            "is_list": true
        }
    },
    "actions": {},
    "examples": [
        {
            "utterance": "when a new xkcd is out",
            "program": "let stream x := monitor (@com.xkcd.get_comic());"
        },
        {
            "utterance": "when a new xkcd is out in the what-if section",
            "program": "let stream x := monitor (@com.xkcd.what_if());"
        },
        {
            "utterance": "the latest xkcd",
            "program": "let table x := @com.xkcd.get_comic();"
        },
        {
            "utterance": "the xkcd number ${p_number}",
            "program": "let table x := \\(p_number :Number) -> @com.xkcd.get_comic(number=p_number);"
        },
        {
            "utterance": "a random xkcd",
            "program": "let table x := @com.xkcd.random_comic();"
        },
        {
            "utterance": "when there is a new post in the xkcd what-if blog",
            "program": "let stream x := monitor (@com.xkcd.what_if());"
        },
        {
            "utterance": "the latest xkcd what if",
            "program": "let table x := @com.xkcd.what_if();"
        },
        {
            "utterance": "a random xkcd comic",
            "program": "let table x := @com.xkcd.random_comic();"
        },
        {
            "utterance": "an xkcd comic at random",
            "program": "let table x := @com.xkcd.random_comic();"
        },
        {
            "utterance": "when a new xkcd is posted",
            "program": "let stream x := monitor (@com.xkcd.get_comic());"
        },
        {
            "utterance": "xkcd",
            "program": "let table x := @com.xkcd.get_comic();"
        },
        {
            "utterance": "xkcd ${p_number:const}",
            "program": "let table x := \\(p_number :Number) -> @com.xkcd.get_comic(number=p_number);"
        }
    ],
    "version": 91,
    "developer": false
}
